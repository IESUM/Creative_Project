<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="449.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="creative_project.Login">

    <children>
        <TextField fx:id="tf_id" layoutX="199.0" layoutY="146.0" prefHeight="40.0" prefWidth="200.0" promptText="아이디">
            <font>
                <Font size="15.0" />
            </font></TextField>
        <PasswordField fx:id="pf_passwd" layoutX="199.0" layoutY="193.0" onKeyPressed="#enter" prefHeight="40.0" prefWidth="200.0" promptText="비밀번호 ">
            <font>
                <Font size="15.0" />
            </font>
        </PasswordField>
        <Button id="login" fx:id="btn_login" layoutX="200.0" layoutY="269.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="200.0" text="로그인" textAlignment="CENTER">
            <font>
                <Font size="19.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <effect>
                <Blend opacity="0.0" />
            </effect>
        </Button>
        <Button fx:id="btn_signup" layoutX="267.0" layoutY="321.0" mnemonicParsing="false" onAction="#signUp" text="회원가입" textFill="#a1a1a1" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
        <Text layoutX="143.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Outdoorgram" textAlignment="CENTER" wrappingWidth="312.0">
            <font>
                <Font size="36.0" />
            </font>
        </Text>
        <Text fx:id="t_result" layoutX="226.0" layoutY="361.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="146.0">
            <font>
                <Font size="15.0" />
            </font></Text>
        <Button fx:id="gogo" layoutX="434.0" layoutY="264.0" mnemonicParsing="false" onAction="#goGo" text="사용자 로그인" />
        <Button fx:id="bt_manager" layoutX="434.0" layoutY="293.0" mnemonicParsing="false" onAction="#goManager" text="관리자 로그인" />
    </children>

</AnchorPane>
